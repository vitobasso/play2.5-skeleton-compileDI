# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET     /form-show                  controllers.CandidateController.showForm
POST    /form-submit                controllers.CandidateController.submitForm
GET     /list                       controllers.CandidateController.list

GET     /upload-page                controllers.CandidateController.uploadPage
POST    /upload                     controllers.CandidateController.upload

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)


